{
    "name": "projectinfiniteme/sextant",
    "description": "Laravel Sextant Engine",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yure Nery",
            "email": "yurenery@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": ">=5.8",
        "doctrine/dbal": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Amondar\\Sextant\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/TestCase.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "~3.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amondar\\Sextant\\SextantServiceProvider"
            ],
            "aliases": {
                "Sextant": "Amondar\\Sextant\\Facade"
            }
        }
    }
}
