{
    "name": "projectinfiniteme/laravel-core-verification-broker",
    "description": "Core verification broker for any verification actions",
    "type": "library",
    "version": "1.1.3",
    "authors": [
        {
            "name": "Yure Nery",
            "email": "yurenery@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0",
        "ajaxray/short-code": "^1.1"
    },
    "autoload": {
